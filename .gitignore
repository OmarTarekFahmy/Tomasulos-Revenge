!.mvn/wrapper/maven-wrapper.jar
!.mvn/wrapper/maven-wrapper.properties
target/
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties
dependency-reduced-pom.xml
buildNumber.properties

# Gradle
.gradle/
/build/
/gradle-app.setting
!gradle/wrapper/gradle-wrapper.jar
!gradle/wrapper/gradle-wrapper.properties

# Eclipse
.metadata/
*.tmp
*.bak
*.swp
*~.nib
/.classpath
/.project
/.settings/
/bin/

# IntelliJ IDEA
/.idea/
/*.iws
/*.iml
/*.ipr
/out/

# NetBeans
/nbproject/private/
/nbbuild/
/dist/
/nbdist/
/.nb-gradle/

# VS Code
.vscode/

# Generated sources
/generated/
/target/generated-sources/
/generated-sources/

# Annotation processors
.factorypath

# Test reports
/surefire-reports/
/failsafe-reports/
/test-outputs/
/build/test-results/
/build/reports/

# Coverage
jacoco.exec
coverage/
*.lcov

# Temporary files
*.tmp
*.temp
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db
desktop.ini
Thumbs.db:encryptable

# IDE-specific local files
.idea/
*.iml

# Credentials and local config
local.properties
*.jks
*.keystore
credentials.properties
*.env
.env.*

# Native image / runtime / packaging
jlink/
jmod/
*.deb
*.rpm
*.msi

# Others
*.class
*.log
*.ctxt

# Keep the gitignore itself tracked
!.gitignore